{
  "manifest_version": 4,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "A simple extension that does something useful.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "My Extension",
    "default_popup": "extension.html"
  }
   "content_scripts": [
    {
      "matches": ["https://*"],
      "js": ["extension.js"]
    }
  ]
}

